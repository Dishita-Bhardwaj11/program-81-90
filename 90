#include <stdio.h>

int main() {
    char str[1000];
    fgets(str, 1000, stdin);

    for (int i = 0; str[i] != '\0'; i++) {
        char ch = str[i];

        if (ch >= 'a' && ch <= 'z')
            ch = ch - 32;  
        else if (ch >= 'A' && ch <= 'Z')
            ch = ch + 32;  

        if (ch == '\n') break;
        printf("%c", ch);
    }

    return 0;
}
