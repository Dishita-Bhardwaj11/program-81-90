#include <stdio.h>

int main() {
    char str[1000];
    fgets(str, 1000, stdin);

    int spaces = 0, digits = 0, special = 0;

    for (int i = 0; str[i] != '\0'; i++) {
        char ch = str[i];

        if (ch == ' ') spaces++;
        else if (ch >= '0' && ch <= '9') digits++;
        else if ((ch >= 'A' && ch <= 'Z') || (ch >= 'a' && ch <= 'z'));
        else if (ch != '\n') special++;
    }

    printf("Spaces=%d, Digits=%d, Special=%d", spaces, digits, special);

    return 0;
}
