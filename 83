#include <stdio.h>

int main() {
    char str[1000];
    fgets(str, 1000, stdin);

    int v = 0, c = 0;

    for (int i = 0; str[i] != '\0'; i++) {
        char ch = str[i];
        if (ch >= 'A' && ch <= 'Z') ch = ch + 32; 

        if (ch >= 'a' && ch <= 'z') {
            if (ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u')
                v++;
            else
                c++;
        }
    }

    printf("Vowels=%d, Consonants=%d", v, c);
    return 0;
}
